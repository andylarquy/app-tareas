{
    "env": {
        "browser": true,
        "node": true,
        "cypress/globals": true
    },
    "parser": "babel-eslint",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:cypress/recommended"
    ],
    "plugins": [
        "cypress"
    ],
    "rules": {
        "no-unused-vars": 1,
        "react/prop-types": 1
    }
}